<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/replace_product"
        android:layout_width="399dp"
        android:layout_height="92dp"
        android:layout_marginBottom="575dp"
        android:layout_marginEnd="3dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="100dp"
        android:text="TextView"
        android:background="@drawable/search_round"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/replace_from"
        android:layout_width="388dp"
        android:layout_height="64dp"
        android:layout_marginBottom="476dp"
        android:layout_marginEnd="14dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="41dp"
        android:background="@drawable/search_round"
        android:text="TextView"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/product_returns_from" />

    <TextView
        android:id="@+id/address_replace"
        android:layout_width="380dp"
        android:layout_height="82dp"
        android:layout_marginBottom="356dp"
        android:layout_marginEnd="23dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="47dp"
        android:background="@drawable/search_round"
        android:text="TextView"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/return_from_users"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/done1"
        android:layout_width="97dp"
        android:layout_height="0dp"
        android:layout_marginBottom="228dp"
        android:layout_marginEnd="153dp"
        android:layout_marginStart="170dp"
        android:background="#232222"
        android:text="done"
        android:textColor="#F1F0F0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.366"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>