<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/PRODUCT_NAME_quick"
        android:layout_width="333dp"
        android:layout_height="69dp"
        android:layout_marginBottom="600dp"
        android:layout_marginEnd="47dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/search_round"
        android:paddingLeft="5dp"
        android:text="product_name"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.372" />

    <TextView
        android:id="@+id/SHOP_PRICE_quick"
        android:layout_width="202dp"
        android:layout_height="46dp"
        android:layout_marginBottom="540dp"
        android:layout_marginEnd="177dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/search_round"
        android:paddingLeft="5dp"
        android:text="Shop_price"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.028"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/STOCK_quick"
        android:layout_width="160dp"
        android:layout_height="46dp"
        android:layout_marginBottom="464dp"
        android:layout_marginEnd="284dp"
        android:background="@drawable/search_round"
        android:paddingLeft="5dp"
        android:text="Stock"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


    <TextView
        android:id="@+id/SHOP_OFFER_quick"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginEnd="27dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/search_round"
        android:paddingLeft="5dp"
        android:text="shop_offer"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.028"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/STOCK_quick" />

    <TextView
        android:id="@+id/POLICY_quick"
        android:layout_width="202dp"
        android:layout_height="46dp"
        android:layout_marginBottom="280dp"
        android:layout_marginEnd="175dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/search_round"
        android:paddingLeft="5dp"
        android:text="policy"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.028"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SHOP_OFFER_quick"
        app:layout_constraintVertical_bias="0.333" />

    <TextView
        android:id="@+id/SHOP_ADRRESS_quick"
        android:layout_width="365dp"
        android:layout_height="64dp"
        android:layout_marginEnd="14dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/search_round"
        android:paddingLeft="5dp"
        android:text="shop_address"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.028"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/POLICY_quick" />

    <Button
        android:id="@+id/check_availability"
        android:layout_width="185dp"
        android:layout_height="50dp"
        android:layout_marginBottom="64dp"
        android:layout_marginStart="160dp"
        android:background="@drawable/shopkeeper_button"
        android:text="Check Availability"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/Shop_no"
        android:layout_width="227dp"
        android:layout_height="47dp"
        android:layout_marginBottom="30dp"
        android:layout_marginEnd="12dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/search_round"
        android:text="Shop_no"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/SHOP_OFFER_quick"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/STOCK_quick"
        app:layout_constraintTop_toBottomOf="@+id/PRODUCT_NAME_quick"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/links_text"
        android:layout_width="350dp"
        android:layout_height="30dp"
        android:layout_marginEnd="21dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="24dp"
        android:autoLink="web"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SHOP_ADRRESS_quick" />


</androidx.constraintlayout.widget.ConstraintLayout>