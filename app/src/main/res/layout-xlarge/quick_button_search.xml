<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/PRODUCT_NAME_quick"
        android:layout_width="649dp"
        android:layout_height="114dp"
        android:layout_marginEnd="119dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="115dp"
        android:background="@drawable/search_round"
        android:paddingLeft="5dp"
        android:text="product_name"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/SHOP_PRICE_quick"
        android:layout_width="413dp"
        android:layout_height="92dp"
        android:layout_marginEnd="353dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="55dp"
        android:background="@drawable/search_round"
        android:paddingLeft="5dp"
        android:text="Shop_price"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PRODUCT_NAME_quick" />

    <TextView
        android:id="@+id/STOCK_quick"
        android:layout_width="295dp"
        android:layout_height="77dp"
        android:layout_marginBottom="77dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/search_round"
        android:paddingLeft="5dp"
        android:text="Stock"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/SHOP_OFFER_quick"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SHOP_PRICE_quick" />


    <TextView
        android:id="@+id/SHOP_OFFER_quick"
        android:layout_width="507dp"
        android:layout_height="98dp"
        android:layout_marginBottom="42dp"
        android:layout_marginEnd="261dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/search_round"
        android:paddingLeft="5dp"
        android:text="shop_offer"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/POLICY_quick"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/POLICY_quick"
        android:layout_width="402dp"
        android:layout_height="83dp"
        android:layout_marginBottom="50dp"
        android:layout_marginEnd="366dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/search_round"
        android:paddingLeft="5dp"
        android:text="policy"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/SHOP_ADRRESS_quick"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/SHOP_ADRRESS_quick"
        android:layout_width="715dp"
        android:layout_height="113dp"
        android:layout_marginBottom="30dp"
        android:layout_marginEnd="51dp"
        android:layout_marginStart="34dp"
        android:background="@drawable/search_round"
        android:paddingLeft="5dp"
        android:text="shop_address"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/links_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/check_availability"
        android:layout_width="286dp"
        android:layout_height="78dp"
        android:layout_marginBottom="68dp"
        android:layout_marginEnd="225dp"
        android:layout_marginStart="289dp"
        android:background="@drawable/shopkeeper_button"
        android:text="Check Availability"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/Shop_no"
        android:layout_width="374dp"
        android:layout_height="80dp"
        android:layout_marginBottom="74dp"
        android:layout_marginEnd="21dp"
        android:layout_marginStart="78dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/search_round"
        android:text="Shop_no"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/SHOP_OFFER_quick"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/STOCK_quick"
        app:layout_constraintTop_toBottomOf="@+id/SHOP_PRICE_quick" />

    <TextView
        android:id="@+id/links_text"
        android:layout_width="718dp"
        android:layout_height="90dp"
        android:layout_marginBottom="34dp"
        android:layout_marginEnd="48dp"
        android:layout_marginStart="34dp"
        android:autoLink="web"
        android:text="TextView"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/check_availability"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>