<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/product_returns_from"
        android:layout_width="660dp"
        android:layout_height="140dp"
        android:layout_marginEnd="110dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="213dp"
        android:background="@drawable/search_round"
        android:text="TextView"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/return_from_users"
        android:layout_width="548dp"
        android:layout_height="109dp"
        android:layout_marginEnd="222dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="93dp"
        android:background="@drawable/search_round"
        android:text="TextView"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/product_returns_from"
        tools:ignore="id" />

    <TextView
        android:id="@+id/address_return"
        android:layout_width="548dp"
        android:layout_height="109dp"
        android:layout_marginEnd="222dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="101dp"

        android:background="@drawable/search_round"
        android:text="TextView"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/return_from_users" />

    <Button
        android:id="@+id/done"
        android:layout_width="178dp"
        android:layout_height="76dp"
        android:layout_marginBottom="355dp"
        android:layout_marginEnd="324dp"
        android:layout_marginStart="298dp"
        android:background="#615D5D"
        android:text="done"
        android:textColor="#FAF6F6"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>